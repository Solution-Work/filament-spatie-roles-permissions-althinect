{
    "name": "solution-work/filament-spatie-roles-permissions-althinect",
    "description": "",
    "keywords": [
        "althinect",
        "filament-spatie-roles-permissions"
    ],
    "homepage": "https://github.com/solution-work/filament-spatie-roles-permissions-althinect",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Solution-Work",
            "email": "info@solution-work.de",
            "role": "Company"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "filament/filament": "^2.0|^2.1",
        "illuminate/support": "^8|^9|^10",
        "spatie/laravel-permission": "^5.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Solution-Work\\FilamentSpatieRolesPermissions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solution-Work\\FilamentSpatieRolesPermissions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "solution-work\\FilamentSpatieRolesPermissions\\FilamentSpatieRolesPermissionsServiceProvider"
            ],
            "aliases": {
                "FilamentSpatieRolesPermissions": "Solution-Work\\FilamentSpatieRolesPermissions\\FilamentSpatieRolesPermissionsFacade"
            }
        }
    }
}
